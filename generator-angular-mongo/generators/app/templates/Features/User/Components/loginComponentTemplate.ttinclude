
<#+ void Generate_Login_ComponentTemplate(){	#>
<button class="btn btn-primary" ng-click="vm.authService.login()">Log In Now</button>

<div ng-if="vm.isAuthenticated">
  <p>Thank you for logging in!</p>
  <button ng-click="vm.authService.logout()">Log Out</button>
</div>
<#+    
  	CreateDirectoryFullPath(clientAppPath + ClientCommon.LoginComponentFilePath);
	CreateFile(clientAppPath + ClientCommon.LoginComponentFilePath + "loginTemplate.html");
}	
#>

